<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="one_video_widget">Only one \" 
        <xliff:g id="name" example="videoplayer">%s</xliff:g>\" is allowed in Home screen.
    </string>  
    <string name="orientation">Cannot detect orientation.</string>  
    <string name="scene">Choose scene</string>  
    <string-array name="scene_name"> 
        <item>Default</item>  
        <item>Work</item> 
    </string-array>
    <string name="state">Current</string>
    <string name="scene_instructions">Set Scene</string>

    <!-- Shortcut Array -->
    <!-- Install shortcut successfully -->
    <string name="shortcut_array_installed"><xliff:g id="count" example="3">%1$d</xliff:g> shortcuts created.</string>
    <!-- Install shortcut failed -->
    <string name="shortcut_install_failed">Shortcut \"<xliff:g id="name" example="Xiaoming">%1$s</xliff:g>\" failed to create.</string>
    <string name="shortcut_array_install_failed"><xliff:g id="count" example="3">%1$d</xliff:g> shortcuts failed to create.</string>
    <!-- Failed reason: duplicate -->
    <string name="shortcut_array_duplicate"><xliff:g id="count" example="3">%1$d</xliff:g> shortcuts already exist.</string>
    
</resources>